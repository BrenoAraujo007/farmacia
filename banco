use farmacia;

create table medicamentos (
id_medicamento int not null auto_increment primary key ,
medicamento varchar(99),
quantidade int,
preco float,
categoria varchar(33),
validade date
);

CREATE TABLE usuarios (
    id INT(11) AUTO_INCREMENT PRIMARY KEY,
    usuario VARCHAR(50) NOT NULL,
    senha VARCHAR(255) NOT NULL
);


insert into usuarios(usuario,senha)values ("teste","teste");

INSERT INTO usuarios (usuario, senha) VALUES ("teste", "HASH_DA_SENHA_AQUI");

select*from medicamentos;
select*from usuarios;
